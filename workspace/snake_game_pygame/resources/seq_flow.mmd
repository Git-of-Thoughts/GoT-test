sequenceDiagram
    participant M as Main
    participant G as Game
    participant S as Snake
    participant F as Food
    participant SB as Scoreboard
    M->>G: start_game()
    loop Game Loop
        G->>S: move()
        G->>S: check_collision()
        G->>F: generate_food()
        G->>SB: update_score()
        G->>G: update_game()
        G->>G: draw_game()
    end
    G->>M: end_game()
