classDiagram
    class Game{
        +start_game()
        +pause_game()
        +resume_game()
        +end_game()
    }
    class Snake{
        +move()
        +eat(Food)
        +grow()
        +die()
    }
    class Food{
        +generate()
    }
    class ScoreBoard{
        +update_score(int)
        +reset_score()
        +get_high_score(): int
    }
